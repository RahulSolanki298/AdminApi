@model IEnumerable<AdminApi.Models.School.StudentPassword>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StudentId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ChoiceId1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ChoiceId2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsActive)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StudentId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ChoiceId1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ChoiceId2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsActive)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.StudentPasswordId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.StudentPasswordId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.StudentPasswordId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
